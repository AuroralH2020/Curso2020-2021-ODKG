#    Object Properties
#################################################################

<http://www.example.com/group07/ontology/organization> rdf:type owl:ObjectProperty ;
                 				       rdfs:range :Organization .
                 				       rdfs:domain :Organization ;
		  				       rdfs:label :"Organization"@IRI-based . 

<http://www.example.com/group07/ontology/participant> rdf:type owl:ObjectProperty ;
                 				       rdfs:range :Participant .
                 				       rdfs:domain :Participant;
		  				       rdfs:label :"Participant"@IRI-based . 

<http://www.example.com/group07/ontology/project> rdf:type owl:ObjectProperty ;
                  				  rdfs:range :ProjectInformation .
               				          rdfs:domain :Participant ;
			                          rdfs:label :"project"@IRI-based . 

<http://www.example.com/group07/ontology/researcher> rdf:type owl:ObjectProperty ;
                  				  rdfs:range :Researcher .
               				          rdfs:domain :Researcher ;
			                          rdfs:label :"researcher"@IRI-based . 

