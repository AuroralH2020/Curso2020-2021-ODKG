  <#TriplesMap4> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "csv\cordis-h2020projects-with-links-snippet.csv" ;
    rml:referenceFormulation ql:CSV 
  ];

  rr:subjectMap [
