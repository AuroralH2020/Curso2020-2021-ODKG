@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix transit: <http://vocab.org/transit/terms/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix wgs84_pos: <http://www.w3.org/2003/01/geo/wgs84_pos#>.
@prefix vocab: <http://www.example.com/group07#> .
@base <http://www.example.com/group07/ns#>.

<#TriplesMap1> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "csv\cordis-h2020projects-with-links.csv" ;
    rml:referenceFormulation ql:CSV 
  ];

  rr:subjectMap [
    rr:template "http://www.example.com/group07/project/{Coordinator}";
    rr:termType rr:IRI; 
    rr:class schema:Coordinator;
  ];

  rr:predicateObjectMap [
    rr:predicateMap [ rr:constant owl:sameAs; rr:termType rr:IRI ];
    rr:objectMap [
      rml:reference "WikiCoordinator";
      rr:termType rr:IRI ]
    ].

<#TriplesMap2> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "csv\cordis-h2020projects-with-links.csv" ;
    rml:referenceFormulation ql:CSV 
  ];

  rr:subjectMap [
    rr:template "http://www.example.com/group07/project/{coordinatorCountry}";
    rr:termType rr:IRI; 
    rr:class schema:coordinatorCountry;
  ];

  rr:predicateObjectMap [
    rr:predicateMap [ rr:constant owl:sameAs; rr:termType rr:IRI ];
    rr:objectMap [
      rml:reference "WikiCountry";
      rr:termType rr:IRI ]
    ].